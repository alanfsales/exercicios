package exercicios.beecrowd;

import java.util.Locale;
import java.util.Scanner;

public class Media31040 {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner input = new Scanner(System.in);
		
		String dados = input.nextLine();
		String[] notas = dados.split(" ");
		
		double N1 = Double.parseDouble(notas[0]);
		double N2 = Double.parseDouble(notas[1]);
		double N3 = Double.parseDouble(notas[2]);
		double N4 = Double.parseDouble(notas[4]);
		
		double media = (N1 + N2 + N3 + N4) / 4;
		System.out.printf("Media: %.1f%n", media);
		
		if(media >= 7 ) {
			System.out.println("Aluno aprovado.");
		}
		
		input.close();
	}
}
